{
  "name": "rabbit-dash-board",
  "version": "1.0.0",
  "description": "RabbitMQ Dashboard Monitoring Tool",
  "private": true,
  "author": "La  Griffe",
  "license": "ISC",
  "workspaces": [
    "front-end",
    "back-end"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "dev:front": "npm run dev --workspace=front-end",
    "dev:back": "npm run dev --workspace=back-end",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=back-end"
  },
  "keywords": [
    "rabbitmq",
    "dashboard",
    "monitoring"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
